   <string notr="true">/* Main Window */
QMainWindow {
	background-color: rgb(70, 19, 85);
    font-family: 'Segoe UI', Arial, sans-serif;
}

/* Buttons */
QPushButton {
    background-color: #6c5ce7;
    color: white;
    border: none;
    border-radius: 8px;
    padding: 10px 20px;
    font-weight: bold;
    font-size: 19px;
    min-width: 100px;
}

QPushButton:hover {
    background-color: #5649c0;
}

QPushButton:pressed {
    background-color: #3d36a0;
}

QPushButton:disabled {
    background-color: #2d2d4d;
    color: #7a7a8c;
}

/* Line Edits */
QLineEdit {
	color: rgb(255, 255, 255);
    background-color: white;
    border: 2px solid #3d3d5a;
    border-radius: 6px;
    padding: 8px 12px;
    font-size: 14px;
    selection-background-color: #6c5ce7;
}

QLineEdit:focus {
    border-color: gray;
}

/* Labels */
QLabel {
    font-size: 19px;
}

QLabel#headerLabel {
    font-size: 24px;
    font-weight: bold;
}

/* Combo Boxes */
QComboBox {
    background-color: #1a1a2e;
    border: 2px solid #3d3d5a;
    border-radius: 6px;
    padding: 8px;
    color: white;
    min-width: 120px;
}

QComboBox:hover {
    border-color: #6c5ce7;
}

QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 30px;
    border-left: 1px solid #3d3d5a;
}

QComboBox QAbstractItemView {
    background-color: #1a1a2e;
    color: white;
    selection-background-color: #6c5ce7;
    outline: none;
}

/* Scroll Bars */
QScrollBar:vertical {
    border: none;
    background: #1a1a2e;
    width: 10px;
    margin: 0;
}

QScrollBar::handle:vertical {
    background: #6c5ce7;
    min-height: 20px;
    border-radius: 4px;
}

QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    height: 0;
}

/* Progress Bar */
QProgressBar {
    border: 1px solid #3d3d5a;
    border-radius: 4px;
    text-align: center;
    color: white;
}

QProgressBar::chunk {
    background-color: #6c5ce7;
    width: 10px;
}

/* Tab Widget */
QTabWidget::pane {
    border: none;
}

QTabBar::tab {
    background: #1a1a2e;
    color: #a29bfe;
    padding: 15px 60px;
    border-top-left-radius: 6px;
    border-top-right-radius: 6px;
    border: 1px solid #3d3d5a;
    border-bottom: none;
}

QTabBar::tab:selected {
    background: #6c5ce7;
    color: white;
    font-weight: bold;
}

/* Table View */
QTableView {
    background-color: #1a1a2e;
    border: 1px solid #3d3d5a;
    gridline-color: #3d3d5a;
    alternate-background-color: #252541;
    selection-background-color: #6c5ce7;
    color: white;
}

QHeaderView::section {
    background-color: #252541;
    color: #a29bfe;
    padding: 8px;
    border: none;
}

/* Menu Bar */
QMenuBar {
    background-color: #0a0a1a;
    color: white;
}

QMenuBar::item:selected {
    background-color: #6c5ce7;
}

QMenu {
    background-color: #1a1a2e;
    border: 1px solid #3d3d5a;
    color: white;
}

QMenu::item:selected {
    background-color: #6c5ce7;
}

/* Status Bar */
QStatusBar {
    background-color: #1a1a2e;
    color: #a29bfe;
    border-top: 1px solid #3d3d5a;
}

/* Tool Tips */
QToolTip {
    background-color: #6c5ce7;
    color: white;
    border: none;
    padding: 5px;
    opacity: 230;
}
QStackedWidget{
	color: white;
}</string>
